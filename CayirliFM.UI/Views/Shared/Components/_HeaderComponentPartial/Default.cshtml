@model List<ResultSocialMediaAcconutDto>

<div class="agileinfo-header">
    <div class="container">
        <div class="agile-logo">
            <h1><a href="index.html"><i class="fa fa-music bounce bounceIn animated" aria-hidden="true"></i> Çayırlı FM</a></h1>
        </div>
        <div class="agileits-w3layouts-icons">
            <div class="social-icon">
                @foreach (var item in Model)
                {
                    <a href="@item.SocialMediaUrl" class="social-button twitter" title="@item.SocialMediaName"><i class="@item.Icon"></i></a>
                }
            </div>
        </div>
        <div class="clearfix"> </div>
    </div>
</div>